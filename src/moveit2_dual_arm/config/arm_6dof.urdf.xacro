<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="arm_6dof">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import arm_6dof urdf file -->
    <xacro:include filename="$(find control_robot)/urdf/dual_arm.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="arm_6dof.ros2_control.xacro" />


    <xacro:arm_6dof_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
